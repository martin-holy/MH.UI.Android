<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:id="@android:id/background">
    <shape android:shape="rectangle">
      <solid android:color="@color/c_static_ba" />
      <stroke android:width="1dp" android:color="@color/c_static_bo"/>
    </shape>
  </item>
  <item android:id="@android:id/progress">
    <clip>
      <shape android:shape="rectangle">
        <gradient
          android:startColor="@color/c_accent2"
          android:endColor="@color/c_accent"
          android:angle="0" />
      </shape>
    </clip>
  </item>
</layer-list>