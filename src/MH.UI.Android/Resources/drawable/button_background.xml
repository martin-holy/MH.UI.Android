<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:a="http://schemas.android.com/apk/res/android">
  <item a:state_pressed="true">
    <shape a:shape="rectangle">
      <solid a:color="@color/c_white3"/>
      <stroke a:width="1dp" a:color="@color/c_static_bo"/>
      <corners a:radius="4dp"/>
    </shape>
  </item>
  <item>
    <shape a:shape="rectangle">
      <solid a:color="@color/gray3"/>
      <stroke a:width="1dp" a:color="@color/c_static_bo"/>
      <corners a:radius="4dp"/>
    </shape>
  </item>
</selector>